<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <Appenders>
        <Console name="console">
            <PatternLayout pattern="[%d][%p][%c]: %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <AsyncRoot level="DEBUG">
            <AppenderRef ref="console"/>
        </AsyncRoot>
    </Loggers>
</Configuration>
